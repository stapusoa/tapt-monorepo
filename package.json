{
  "name": "TruBalance Bodywork",
  "version": "1.0.0",
  "description": "TruBalance Bodywork in Orem, Utah provides massage therapy and energy work tailored to restore your body and uplift your spirit.",
  "main": "index.js",
  "private": true,
  "packageManager": "pnpm@8.9.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "deploy:cms": "git subtree push --prefix=apps/cms cms main",
    "prepare": "if [ ! \"$CI\" ]; then husky install; fi",
    "build": "pnpm turbo run build",
    "dev:backend": "pnpm --filter backend dev",
    "build:backend": "pnpm --filter backend build",
    "start:backend": "pnpm --filter backend start",
    "dev:web": "pnpm --filter web dev",
    "build:web": "pnpm --filter web build",
    "start:web": "pnpm --filter web preview",
    "release": "semantic-release",
    "lint": "pnpm eslint . --ext .js,.ts,.jsx,.tsx",
    "lint:fix": "pnpm eslint . --ext .js,.ts,.jsx,.tsx --fix",
    "test": "vitest run",
    "check": "pnpm lint && pnpm test && pnpm build"
  },
  "lint-staged": {
    "*.{js,ts,tsx,jsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "massage",
    "massage therapy",
    "massage in Utah",
    "massage therapy in Utah",
    "massage Utah",
    "massage therapy Utah",
    "Utah massage",
